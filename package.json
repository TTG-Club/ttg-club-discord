{
  "name": "ttg-club-discord",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.26.2",
  "description": "TTG Club Discord Bot",
  "license": "MIT",
  "keywords": [],
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "start": "pnpm run build && node dist/index.js",
    "build": "pnpm run clean && tsc --project tsconfig.json",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "lint:check": "eslint . --cache --cache-strategy content",
    "lint:write": "eslint . --cache --cache-strategy content --fix",
    "lint:workflow": "eslint . --cache --cache-strategy content --quiet"
  },
  "dependencies": {
    "@discordjs/rest": "^2.6.0",
    "@truto/turndown-plugin-gfm": "^1.0.2",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "colord": "^2.9.3",
    "dice-roller-parser": "^0.1.8",
    "discord.js": "^14.25.1",
    "discordjs-button-embed-pagination": "^5.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsdom": "^27.3.0",
    "lodash-es": "^4.17.22",
    "sanitize-html": "^2.17.0",
    "turndown": "^7.2.2"
  },
  "devDependencies": {
    "@svifty7/eslint-config": "^0.0.2",
    "@types/express": "^5.0.6",
    "@types/jsdom": "^27.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.3",
    "@types/sanitize-html": "^2.16.0",
    "@types/turndown": "^5.0.6",
    "eslint": "^9.39.2",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  }
}
